{
  "name": "aom-api-server",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc --rootDir src --outDir dist",
    "build-prod": "tsc --rootDir src --outDir dist && cross-env NODE_ENV=production APP_ENVIRONMENT=production webpack -p --progress",
    "build-server": "gulp transpile-server",
    "build-staging": "gulp transpile-server && cross-env NODE_ENV=staging APP_ENVIRONMENT=staging webpack",
    "serve": "cross-env NODE_ENV=development APP_ENVIRONMENT=development webpack-dev-server --inline",
    "start-staging": "$(npm bin)/cross-env NODE_ENV=staging, CONFIG=./staging.config.yml $(npm bin)/pm2 start dist/server/index.js --name api",
    "test-api": "NODE_PATH=./ mocha --compilers js:babel-core/register test/api/*.js",
    "ttab": "ttab -w",
    "watch-server": "gulp",
    "webpack": "webpack --progress"
  },
